{
  "name": "sb_reentry_app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "analyze": "yarn webpack-bundle-analyzer ./.next/webpack-stats.json -r ./.next/report.html -m static",
    "aptible-deploy": "git push aptible HEAD:master",
    "docker-build": "next build",
    "build": "PROD_STATS=true next build",
    "build_data": "docker compose -f ./docker_postgres.yml up -d",
    "build_with_source_maps": "NODE_OPTIONS=--max_old_space_size=4096 PROD_SRC_MAPS=true next build",
    "code-check": "yarn lint-es && yarn test && yarn type-check && yarn lint-css && yarn format",
    "dev": "next dev",
    "format": "prettier ./**/*.{js,ts,tsx,json,css} --check --ignore-unknown",
    "lint-css": "stylelint ./**/*.css --fix --cache && echo 'All stylelint checks passed!'",
    "lint-es": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx --fix --cache && echo 'All ESLint checks passed!'",
    "start": "next start",
    "start_data": "docker start sb_reentry_app_postgres_1",
    "test": "jest --cache --coverage --silent && echo 'All Jest tests passed!'",
    "type-check": "tsc && echo 'All TypeScript compiler checks passed!'"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",

