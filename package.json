    "stylelint": "^13.9.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^4.1.3",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-stats-plugin": "^1.0.3"
  },
  "resolutions": {
    "**/**/object-path": "^0.11.5",
    "**/**/ini": "^1.3.6",
