    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sequelize": "^6.6.5",
    "styled-components": "^5.3.0",
    "twilio": "^3.67.0",
    "uuid": "^8.3.2",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
